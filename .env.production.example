# =====================================================
# PG19 Client Portal - Production Environment
# =====================================================
# Copy to .env on the server: cp .env.production.example .env
# =====================================================

# Supabase (Public)
SUPABASE_URL=https://supabase.doka.team
SUPABASE_KEY=your_supabase_anon_key

# Supabase (Server-only - SECRET!)
SUPABASE_SECRET_KEY=your_supabase_service_role_key

# Telegram Bot
TELEGRAM_BOT_USERNAME=PG19CONNECTBOT
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Yandex Maps
YANDEX_MAPS_API_KEY=your_yandex_maps_api_key

# DaData (address suggestions)
DADATA_API_KEY=your_dadata_api_key
DADATA_SECRET_KEY=your_dadata_secret_key

# OpenAI (AI bot)
OPENAI_API_KEY=your_openai_api_key

# Beeline (call verification)
BEELINE_API_TOKEN=your_beeline_api_token

# Telegram Web App URL
TWA_URL=https://pg19-tg.doka.team
